#include <iostream>
using namespace std;

// Overloaded function to find the maximum of two integers
int maximum(int a, int b) {
    return (a > b) ? a : b;
}

// Overloaded function to find the maximum of two floating-point numbers
float maximum(float a, float b) {
    return (a > b) ? a : b;
}

// Overloaded function to find the maximum of two characters
char maximum(char a, char b) {
    return (a > b) ? a : b;
}

int main() {
    int int1 = 5, int2 = 10;
    float float1 = 5.5, float2 = 10.2;
    char char1 = 'A', char2 = 'B';

    // Finding maximum of integers
    cout << "Maximum of integers: " << maximum(int1, int2) << endl;

    // Finding maximum of floats
    cout << "Maximum of floats: " << maximum(float1, float2) << endl;

    // Finding maximum of characters
    cout << "Maximum of characters: " << maximum(char1, char2) << endl;

    return 0;
}
