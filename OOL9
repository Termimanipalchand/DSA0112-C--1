#include <iostream>
using namespace std;

int add(int a, int b) {
    return a + b;
}
float add(float a, float b) {
    return a + b;
}

int main() {
    int int1 = 5, int2 = 10;
    float float1 = 5.5, float2 = 10.2;

    cout << "Sum of integers: " << add(int1, int2) << endl;
    cout << "Sum of floats: " << add(float1, float2) << endl;

    return 0;
}
